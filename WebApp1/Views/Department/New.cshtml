@model Department 
@{
    ViewData["Title"] = "New";
}

<h1>New</h1>
<!--/Department/SaveNew?Name=fsdf&ManagerName=Ahemd-->
<form action="/Department/SaveNew" method="post">
    <div>
        <label>Department Name</label>
        <input name="Name" id="Name" value="@Model?.Name" type="text" class="form form-control" />
    </div>    
    <div>
        <label>Manager Name</label>
        <input name="ManagerName" id="ManagerName" value="@Model?.ManagerName" type="text" class="form form-control" />
    </div>
    <input type="submit" value="Create" class="btn btn-success" />
</form>
<a href="/Department/Index">Back To List</a>